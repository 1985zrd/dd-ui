{
  "name": "dd-ui-vue",
  "version": "0.1.5",
  "description": "A Personal Learning UI library For Vue",
  "author": "zrd",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "clean": "rimraf lib",
    "dist": "npm run clean && npm run lib:all && npm run lib:disp",
    "lib:all": "webpack --config ./build/webpack.lib.prod.js",
    "lib:disp": "webpack --config ./build/webpack.lib.prod.disperse.js"
  },
  "main": "lib/index.js",
  "dependencies": {
    "async-validator": "^3.3.0",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-router": "^3.3.1"
  },
  "devDependencies": {
    "@intervolga/optimize-cssnano-plugin": "^1.0.6",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "^4.4.4",
    "@vue/cli-service": "~4.4.0",
    "@vue/test-utils": "^1.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "markdown-it": "^12.3.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "prismjs": "^1.20.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/1985zrd/dd-ui/issues"
  },
  "homepage": "https://github.com/1985zrd/dd-ui#readme",
  "keywords": [
    "UI",
    "Vue",
    "UI-Library"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1985zrd/dd-ui.git"
  }
}
